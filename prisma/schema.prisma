// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @id @default(autoincrement())
  createdAt DateTime @default(now())
  email     String?
  name      String?
  sentence  String?
  metric    Float?
  comment   String?
  model     String?
}

model Sentences {
  id    Int     @id @default(autoincrement())
  createdAt DateTime @default(now())
  sentence  String?
}